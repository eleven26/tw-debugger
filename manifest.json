{
  "name": "tw-debugger",
  "version": "2.0.6.0",
  "description": "重定向api请求到开发者服务器",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "cookies",
    "tabs",
    "http://*.86yqy.com/*"
  ],
  "icons": {
    "16": "logos/16x16.png",
    "48": "logos/48x48.png",
    "128": "logos/128x128.png"
  },
  "commands": {
    "open-cart": {
      "suggested_key": {
        "windows": "Alt+C",
        "mac": "Alt+C"
      },
      "description": "Open shopping cart in new page."
    },
    "open-develop-page": {
      "suggested_key": {
        "windows": "Alt+D",
        "mac": "Alt+D"
      },
      "description": "Open developer page."
    }
  },
  "background": {
    "scripts": [
      "background.js",
      "create_menus.js"
    ]
  },
  "manifest_version": 2
}